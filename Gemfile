source "https://rubygems.org"

gem "jekyll-theme-chirpy", "~> 7.4"

gem "html-proofer", "~> 5.0", group: :test

# Windows does not include zoneinfo files, so bundle the tzinfo-data gem
platforms :mingw, :x64_mingw, :mswin, :jruby do
  gem "tzinfo", ">= 1", "< 3"
  gem "tzinfo-data"
end

gem "webrick", "~> 1.8"
